<plugin>

   <extension point="org.eclipse.net4j.util.productGroups">
      <productGroup id="org.eclipse.emf.cdo.lm.server.lifecycleManagers" name="%productGroup.name"/>
   </extension>

   <extension point="org.eclipse.net4j.util.factories">
      <factory productGroup="org.eclipse.emf.cdo.lm.server.lifecycleManagers"
               type="default"
               class="org.eclipse.emf.cdo.lm.server.XMLLifecycleManager$Factory"/>
   </extension>
   
   <extension point="org.eclipse.emf.cdo.server.appExtensions">
      <appExtension class="org.eclipse.emf.cdo.lm.server.LMAppExtension"/>
   </extension>

</plugin>
